{
  "hosting": {
    "redirects": [
      {
        "source": "/about",
        "destination": "/",
        "type": 301
      },
      {
        "source": "/contact",
        "destination": "/",
        "type": 302
      }
    ],
    "public": "dist",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "**",
        "destination": "/index.html"
      }
    ]
  }
}
